{
  "name": "sitegeist/goldengate-dto",
  "description": "DTOs and Serializer used for the Sitegeist GoldenGate Neos-Shopware Bridge",
  "type": "library",
  "homepage": "http://www.sitegeist.de",
  "license": ["MIT"],
  "require": {
    "php": "~7.0",
    "symfony/serializer": "~3.0",
    "symfony/property-access": "~3.0",
    "symfony/property-info": "~3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~6.0"
  },
  "autoload": {
    "psr-4": {
      "Sitegeist\\Goldengate\\Dto\\": "Classes"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Sitegeist\\Goldengate\\Dto\\Tests\\": "Tests"
    }
  }
}